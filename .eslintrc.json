{
   "env": {
      "browser": true
   },
   "parser": "babel-eslint",
   "rules": {
      "indent": [
         "error",
         3,
         {
            "SwitchCase": 1
         }
      ],
      "brace-style": [
         "error",
         "stroustrup"
      ],
      "react/jsx-indent": [
         "error",
         3
      ],
      "react/jsx-indent-props": [
         "error",
         3
      ],
      "react/jsx-filename-extension": "off",
      "no-underscore-dangle": "off",
      "no-shadow": "off",
      "implicit-arrow-linebreak": "off",
      "lines-between-class-members": ["error", "always", {
         "exceptAfterSingleLine": true
      }]
   },
   "extends": [
      "airbnb"
   ],
   "plugins": [
      "react"
   ]
}
